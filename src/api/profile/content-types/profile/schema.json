{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "integer"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "profile"
    },
    "age": {
      "type": "integer"
    },
    "imageURL": {
      "type": "string"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Not to say"
      ]
    }
  }
}
